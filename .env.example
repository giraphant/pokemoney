PRIVATE_KEY=your-base58-encoded-private-key
JUP_API_KEY=your-jupiter-api-key
DRY_RUN=true

# Trading pairs - format: TOKENA/TOKENB:multiplier,...
PAIRS=JLP/SOL:1,JLP/USDC:1,JLP/USDT:1

# Amount tiers in USD to try per pair
AMOUNT_TIERS=5000,2000,1000,500

# Expected value per card in USD
CARD_EV_USD=2.1

# Max residual imbalance in USD before rebalancing
MAX_RESIDUAL_USD=100

# Scan interval in milliseconds
SCAN_INTERVAL_MS=10000

# Price cache TTL in milliseconds
PRICE_CACHE_TTL_MS=30000
